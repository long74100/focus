{
    "name": "Hello Extensions",
    "version": "1.0",
    "description" : "Base Level Extension",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    },
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/logo.png",
        "32": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
      }
    },
    "options_page": "options.html",
    "icons": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  }